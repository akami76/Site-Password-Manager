{
    "manifest_version": 3,
    "name": "Site Password Manager",
    "version": "1.2",
    "description": "A simple password manager for websites.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": [
      "storage",
      "clipboardWrite",
      "activeTab"
    ]
  }
  